<Page x:Class="Client.windows.EditUsers"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Client.windows"
      mc:Ignorable="d" 
      d:DesignHeight="350" d:DesignWidth="250"
      Title="EditUsers">

    <Grid>
        <TextBox x:Name="userNameTextBox"
                 HorizontalAlignment="Left" 
                 Height="23" 
                 Margin="24,38,0,0" 
                 TextWrapping="Wrap" 
                 VerticalAlignment="Top"
                 Width="194" 
                 TextChanged="userNameTextBox_TextChanged" />
        <ListBox x:Name="userNameDropDown" 
                 HorizontalAlignment="Left" 
                 Height="23" Margin="24,66,0,0" 
                 VerticalAlignment="Top"
                 Width="194"
                 Visibility="Visible" 
                 KeyDown="userNameDropDown_KeyDown" />
    </Grid>
</Page>
